
<h1 class="text-center">Enter pet information</h1>

<div class="row">
  <div class="panel panel-default">
    <div class="col-md-5">
      <%= form_for @pet, class: 'form-horizontal' do |f| %>
          <%= render partial: 'form', locals: { f: f, pet: @pet } %>
      <% end %>
    </div>
  </div>

</div>

<br><%= link_to 'Go Back', pets_path %>